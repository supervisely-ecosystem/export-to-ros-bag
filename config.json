{
  "name": "Export Point Clouds to ROS Bag",
  "type": "app",
  "version": "2.0.0",
  "categories": ["pointclouds", "export"],
  "context_menu": {
    "target": [
      "point_cloud_project",
      "point_cloud_dataset",
      "point_cloud_episodes_project",
      "point_cloud_episodes_dataset"
    ],
    "context_root": "Download as"
  },
  "modal_template": "src/modal.html",
  "modal_template_state": {
    "annMode": "sly",
    "mergeMode": false,
    "fillPoints": true,
    "fillRate": "50"
  },
  "modal_width": 700,
  "description": "Export Pointclouds and Pointcloud episodes to ROS Bag format",
  "docker_image": "supervisely/import-export:6.73.78",
  "main_script": "src/main.py",
  "task_location": "workspace_tasks",
  "headless": true,
  "icon_cover": true,
  "icon": "https://github.com/supervisely-ecosystem/export-to-ros-bag/releases/download/v0.0.1/export_ros_logo.png",
  "poster": "https://github.com/supervisely-ecosystem/export-to-ros-bag/releases/download/v0.0.1/export_ros_poster.png"
}
